{
  "name": "jaf",
  "version": "1.0.0",
  "ignore": [
    "**/.*",
    "node_modules",
    "components",
    "bower_components",
    "bower_modules",
    "test",
    "tests"
  ],
  "exportsOverride": {
    "angular": {
      "scripts/libs": "angular.js",
      "styles/libs": "angular-csp.css"
    },
    "angular-ui-router": {
      "scripts/libs": "release/angular-ui-router.js"
    },
    "angular-ui-select": {
      "scripts/libs": "dist/select.js",
      "styles/libs": "dist/select.css"
    },
    "angular-ui-utils": {
      "scripts/libs": "ui-utils.js"
    },
    "sweetalert": {
      "scripts/libs": "lib/sweet-alert.js",
      "styles/libs": "lib/sweet-alert.css"
    },
    "jquery": {
      "scripts/libs": "dist/jquery.js"
    },
    "modernizr": {
      "scripts/libs": "modernizr.js"
    },
    "pubsub-js": {
      "scripts/libs": "src/pubsub.js"
    },
    "normalize-less": {
      "styles/mixins": "normalize.less"
    },
    "lesshat": {
      "styles/mixins": "build/lesshat.less"
    },
    "animate-less": {
      "styles/mixins": "less/*.less"
    },
    "font-awesome": {
      "styles/mixins": "less/**/*.less",
      "fonts/": "fonts/**/*"
    },
    "*": {
      "scripts/libs": "**/*",
      "styles/libs": "**/*",
      "images/gui/": "**/*",
      "fonts/": "**/*"
    }
  },
  "dependencies": {
    "normalize-less": "*",
    "jquery": "~2.1.1",
    "modernizr": "~2.8.3",
    "pubsub-js": "~1.5.0",
    "font-awesome": "~4.2.0",
    "lesshat": "~3.0.2",
    "angular-ui-select": "~0.7.0",
    "angular-ui-utils": "~0.1.1",
    "angular-ui-router": "~0.2.11",
    "animate-less": "~2.0.1"
  },
  "devDependencies": {}
}
